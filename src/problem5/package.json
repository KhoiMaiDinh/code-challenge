  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node build/app.js",
    "start:dev": "nodemon --watch src --ext ts,js --exec \"ts-node -r tsconfig-paths/register src/app.ts\" --legacy-watch",
    "start:prod": "npm run build && npm run start",
    "migrate:create": "npx migrate-mongo create",
    "migrate:up": "npx migrate-mongo up",
    "migrate:down": "npx migrate-mongo down",
    "migrate:status": "npx migrate-mongo status",
    "seed": "ts-node -r tsconfig-paths/register scripts/seed-all.ts",
    "test": "jest",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\""
  },
  "keywords": [],
